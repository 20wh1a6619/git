
#include <iostream>
#include <vector>
#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,k;
    cin>>n;
    vector<int>vect(n);
    for(int i=0;i<n;i++){
        cin>>vect[i];
    }
    cin>>k;
    reverse(vect.begin(),vect.end());
    reverse(vect.begin(),vect.begin()+k);
    reverse(vect.begin()+k,vect.begin()+n);
    for(int i=0;i<n;i++){
        cout<<vect[i]<< " ";
    }
}
   
